{
  "name": "themepark",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn run dev:electron",
    "webpack-serve": "webpack serve",
    "webpack-watch": "webpack -w",
    "dev:electron": "cross-env NODE_ENV=development TARGET=electron-renderer concurrently -n webpack,electron 'yarn:webpack-serve' \"wait-on http://localhost:8080 && electron .\"",
    "dev:web": "cross-env NODE_ENV=development TARGET=web concurrently -n web,api 'yarn:webpack-serve --open' 'yarn:backend'",
    "backend": "nodemon --watch backend backend/index.js",
    "electron": "electron .",
    "format": "prettier --write src index.js server.js",
    "mongodb:dump": "mongodump --db=themepark --archive=testdata.archive",
    "mongodb:restore": "mongo themepark --eval \"db.dropDatabase()\" && mongorestore  --archive=testdata.archive --uri=\"mongodb://localhost:27017\" --nsInclude=\"themepark.*\" -d themepark"
  },
